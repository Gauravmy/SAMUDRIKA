{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "env": {
    "MODEL_API_KEY": "@model_api_key",
    "FLASK_API_URL": "@flask_api_url"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
